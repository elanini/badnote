{
  "manifest_version": 2,
  "name": "Bad Note",
  "version": "0.1.9",
  "update_url": "https://elanini.com/update/updates.xml",
  "content_scripts": [
    {
      "matches": [
        "https://piazza.com/class*"
      ],
      "js": [
        "jquery-3.3.1.min.js",
        "content.js"
      ],
      "run_at": "document_start"
    }
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "http://localhost/*"
  ],
  "applications": {
    "gecko": {
      "id": "{512c070e-2dc6-487f-945b-f39c850ef92b}",
      "strict_min_version": "42.0",
      "update_url": "https://elanini.com/update/updates.json"
    }
  },
  "icons": {
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "homepage_url": "https://github.com/elanini/badnote",
  "author": "Morgan Grant",
  "description": "Make Piazza mean"
}
