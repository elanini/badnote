{
  "manifest_version": 2,
  "name": "Bad Note",
  "version": "0.1",
   "browser_action": {
   },
  "content_scripts": [
    {
      "matches": [
        "https://piazza.com/class/*"
      ],
      "js": ["jquery-3.3.1.min.js", "content.js"],
      "run_at": "document_start"
    }
  ],
  "background": {
    "scripts": ["background.js"]
  },
  "permissions": [
    "http://localhost/*"
  ]
}
